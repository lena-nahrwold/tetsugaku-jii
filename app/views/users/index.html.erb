<div class="container">
  <div class="row">
    <h3>Users</h3>
      <table class="table">
        <tbody>
          <% @users.order('created_at DESC').each do |user| %>
            <tr>
              <%= render user if user.confirmed_at != nil %>
            </tr>
          <% end %>
        </tbody>
      </table>
  </div>
</div>
